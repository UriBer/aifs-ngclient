{
  "name": "aifs-client",
  "version": "0.1.0",
  "description": "Cross-Cloud Object-Management Client with AIFS support",
  "main": "dist/main/main/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:main\"",
    "dev:main": "tsc -p tsconfig.main.json -w",
    "dev:renderer": "vite",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "vite build",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "jest",
    "test:aifs": "ts-node --transpile-only --project tsconfig.test.json scripts/test-aifs-provider.ts",
    "mock:aifs": "node scripts/mock-aifs-server.js"
  },
  "keywords": [
    "aifs",
    "file-manager",
    "s3",
    "gcs",
    "azure",
    "electron"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^5.0.3",
    "concurrently": "^8.0.1",
    "electron": "^22.3.27",
    "electron-builder": "^23.6.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.7"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.321.1",
    "@aws-sdk/lib-storage": "^3.321.1",
    "@azure/storage-blob": "^12.14.0",
    "@google-cloud/storage": "^6.10.1",
    "@grpc/grpc-js": "^1.8.14",
    "@grpc/proto-loader": "^0.7.6",
    "axios": "^1.5.0",
    "blake3": "^2.1.7",
    "electron-store": "^8.1.0",
    "keytar": "^7.9.0",
    "minimatch": "^10.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
